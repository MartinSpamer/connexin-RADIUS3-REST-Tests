      </param>
      <method name="GET" id="GET">
        <doc xml:lang="en" title="GET">Returns a list of Accounting Entries</doc>
        <request>
          <param name="user" default="${#TestCase#userId}" type="xs:integer" required="false" style="query" xmlns:xs="http://www.w3.org/2001/XMLSchema">
