            <option value="1"/>
          </param>
          <param name="limit" default="${#TestCase#limit}" type="xs:integer" required="false" style="query" xmlns:xs="http://www.w3.org/2001/XMLSchema">
            <doc xml:lang="en" title="limit Parameter">limit - The maximum number of records to retrieved per page (default: 25)</doc>
            <option value="${#TestCase#PageLimit}"/>
            <option value="25"/>
          </param>
        </request>
        <response status="200">
          <representation mediaType="application/json"/>
          <representation mediaType="application/json; charset=UTF-8"/>
        </response>
        <response status="400">
          <representation mediaType="text/html; charset=UTF-8"/>
